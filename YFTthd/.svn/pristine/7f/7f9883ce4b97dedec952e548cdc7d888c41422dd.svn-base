<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@color/bg_grey"
    >
    <include layout="@layout/merge_jacktitle"/>
        
    
    <LinearLayout
        android:id="@+id/table_person"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/common_margin_bigger"
        android:orientation="vertical" >
	</LinearLayout>
    
	<Button 
	    android:id="@+id/btn_person_logout"
	    android:layout_width="match_parent"
	    android:layout_height="@dimen/v_height_edit"
	    android:text="退出当前账户"
	    android:textColor="@android:color/white"
	    android:textSize="@dimen/textsize_r_bigger"
	    android:background="@drawable/btn_person_logout"
	    android:layout_marginTop="@dimen/activity_vertical_margin"
	    android:layout_marginBottom="@dimen/common_margin_bigger"
	    android:layout_marginLeft="@dimen/common_margin"
	    android:layout_marginRight="@dimen/common_margin"
	    />
    
</LinearLayout>
